{
  "name": "@universal-middleware/hattip",
  "version": "0.4.11",
  "type": "module",
  "description": "Hono adapter for universal middlewares",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
