{
  "name": "@universal-middleware/hattip",
  "version": "0.2.2",
  "type": "module",
  "description": "Hono adapter for universal middlewares",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "author": "JoÃ«l Charles <joel.charles91@gmail.com>",
  "repository": "https://github.com/magne4000/universal-handler",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "test": "vitest run",
    "test:run-hattip:node": "tsx tests/entry-hattip.ts",
    "test:run-hattip:bun": "bun tests/entry-hattip.ts",
    "test:run-hattip:worker": "wrangler dev tests/entry-hattip-cloudflare.ts",
    "test:run-hattip:deno": "deno run --unstable-byonm --unstable-sloppy-imports -A tests/entry-hattip.ts",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "release": "LANG=en_US release-me patch",
    "release:minor": "LANG=en_US release-me minor",
    "release:commit": "LANG=en_US release-me commit"
  },
  "dependencies": {
    "@universal-middleware/core": "^0.2.6"
  },
  "devDependencies": {
    "@brillout/release-me": "^0.4.0",
    "@hattip/adapter-bun": "^0.0.47",
    "@hattip/adapter-cloudflare-workers": "^0.0.47",
    "@hattip/adapter-deno": "^0.0.47",
    "@hattip/adapter-node": "^0.0.47",
    "@hattip/compose": "^0.0.47",
    "@hattip/core": "^0.0.47",
    "@hattip/cors": "^0.0.47",
    "@hattip/router": "^0.0.47",
    "@swc/core": "^1.7.11",
    "@types/node": "^20.14.10",
    "@universal-middleware/tests": "workspace:*",
    "rimraf": "^6.0.0",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "wrangler": "^3.72.3"
  },
  "sideEffects": false,
  "keywords": [
    "hattip",
    "universal-middleware",
    "middleware",
    "handlers",
    "server-adapters",
    "hattip-middleware",
    "universal-server",
    "context-middleware",
    "request-handling"
  ]
}
