    "@hattip/adapter-node": "^0.0.49",
    "@hattip/compose": "^0.0.49",
    "@hattip/core": "catalog:",
    "@hattip/cors": "^0.0.49",
    "@hattip/router": "catalog:",
    "@swc/core": "catalog:",
    "@types/node": "catalog:",
    "@universal-middleware/tests": "workspace:*",
    "rimraf": "catalog:",
    "tsup": "catalog:",
